<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Experimental.Bindthing.ListViewPage"
             xmlns:ns="clr-namespace:Experimental.Bindthing">
    <ListView x:Name="MyListView">
        <ListView.Header>
            <StackLayout>
                <Label Text="{Binding One}" HorizontalOptions="Center" />
                <ns:ContainedLabel Two="{Binding One}" />
            </StackLayout>
        </ListView.Header>
    </ListView>
</ContentPage>
